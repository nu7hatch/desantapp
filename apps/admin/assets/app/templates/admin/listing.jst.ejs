<h2><i class="icon-<%= icon %>"></i> <%= title %></h2>
<div id="counter"></div>
<div class="action_buttons"></div>
<div class="subnav"></div>

<div class="extras"></div>

<div class="listing_wrapper">
  <table class="listing" id="<%= id %>" cellspacing="0" cellpadding="0">
    <thead>
      <tr>
        <% _.each(columns, function(column) { %>
          <th><%= column %></th>
        <% }) %>
      </tr>
    </thead>
    <tbody>
      <div class="empty_screen">
      </div>
    </tbody>
  </table>
</div>
